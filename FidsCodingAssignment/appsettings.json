{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "App": {
    "Cache": {
      "RetentionDays": 3
    }
  },
  "Flight": {
    "BoardingWindowInMinutes": 15,
    "ThresholdInMinutes": 30,
    "Delta": 2
  }
}
