{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "App": {
    "Cache": {
      "RetentionDays": 3
    }
  },
  "Flight": {
    "PreDefinedWindowInMinutes": 30,
    "DeltaThreshold": 10,
    "AddtionalDepartureTimeInMinutes": 0
  }
}
